{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "config": "./config",
          "lib": "./src/lib",
          "client": "./src/client",
          "views": "./src/client/views",
          "styles": "./src/client/views/styles",
          "components": "./src/client/views/components"
        }
      }
    ]
  ],
  "ignore": [
    "node_modules/**"
  ]
}